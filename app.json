{
  "expo": {
    "name": "MySchool International",
    "slug": "MySchoolParent",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/3c5b0113-0632-44e7-aa90-959be73c85f8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "buildNumber": "2",
      "bundleIdentifier": "com.moc.myschoolParentIos",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "Allow myschoolParentIos client app to use your location for better job assignment",
        "NSLocationUsageDescription": "Allow myschoolParentIos client app to use your location for better job assignment",
        "NSLocationAlwaysUsageDescription": "Allow myschoolParentIos client app to use your location for better job assignment",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow myschoolParentIos client app to use your location for better job assignment"
      }
    },
    "android": {
      "package": "com.moc.myschoolParent",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3c5b0113-0632-44e7-aa90-959be73c85f8"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
